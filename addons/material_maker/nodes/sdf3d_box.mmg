	"shader_model": {
		"code": "",
		"global": [
			"float box3d(vec3 uv, vec3 s) {",
			"\tvec3 q = abs(uv) - s;",
			"\treturn length(max(q, 0.0))+min(max(q.x, max(q.y, q.z)), 0.0);",
			"}",
			""
		],
		"inputs": [

		],
		"instance": "",
		"longdesc": "Generates a rounded box as a signed distance function",
		"name": "Box",
		"outputs": [
			{
				"longdesc": "Shows the rounded box",
				"sdf3d": "box3d($uv, vec3($sx, $sy, $sz))-$r",
				"shortdesc": "Output",
				"type": "sdf3d"
			}
		],
		"parameters": [
			{
				"control": "Rect1.x",
				"default": 0.3,
				"label": "Size X",
				"longdesc": "The size of the box along the X axis",
				"max": 1,
				"min": 0,
				"name": "sx",
				"shortdesc": "Size.x",
				"step": 0.01,
				"type": "float"
			},
			{
				"control": "Rect1.y",
				"default": 0.25,
				"label": "Size Y",
				"longdesc": "The size of the box along the Y axis",
				"max": 1,
				"min": 0,
				"name": "sy",
				"shortdesc": "Size.y",
				"step": 0.01,
				"type": "float"
			},
			{
				"control": "None",
				"default": 0.25,
				"label": "Size Z",
