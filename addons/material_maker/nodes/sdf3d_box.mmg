				"max": 1,
				"min": 0,
				"name": "sz",
				"shortdesc": "Size.z",
				"step": 0.01,
				"type": "float"
			},
			{
				"control": "None",
				"default": 0.01,
				"label": "Radius",
				"longdesc": "The radius of the rounded box",
				"max": 1,
				"min": 0,
				"name": "r",
				"shortdesc": "Radius",
