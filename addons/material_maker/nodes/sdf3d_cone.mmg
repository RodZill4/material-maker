		"y": 0
	},
	"parameters": {
		"a": 30,
		"axis": 2
	},
	"shader_model": {
		"code": "",
		"global": "",
		"inputs": [

		],
		"instance": "",
		"name": "Cone",
		"outputs": [
			{
				"sdf3d": "dot(vec2(cos($a*0.01745329251),sin($a*0.01745329251)),vec2($axis))",
