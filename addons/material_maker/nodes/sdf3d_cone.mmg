			}
		],
		"parameters": [
			{
				"default": 5,
				"label": "Axis",
				"name": "axis",
				"type": "enum",
				"values": [
					{
						"name": "+X",
						"value": "length($uv.yz),-$uv.x"
					},
					{
						"name": "-X",
						"value": "length($uv.yz),$uv.x"
					},
					{
						"name": "+Y",
						"value": "length($uv.xz),$uv.y"
					},
					{
						"name": "-Y",
						"value": "length($uv.xz),-$uv.y"
					},
					{
						"name": "+Z",
						"value": "length($uv.xy),-$uv.z"
					},
					{
						"name": "-Z",
						"value": "length($uv.xy),$uv.z"
					}
				]
			},
			{
				"control": "None",
				"default": 30,
				"label": "Angle",
				"max": 90,
				"min": 0,
				"name": "a",
				"step": 1,
				"type": "float"
			}
		]
	},
	"type": "shader"
}