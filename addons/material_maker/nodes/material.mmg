		"preview_shader": [
			"shader_type spatial;",
			"render_mode blend_mix,cull_back,diffuse_burley,specular_schlick_ggx",
			"$if $(connected:opacity_tex) and $(param:flags_transparent)",
			",depth_prepass_alpha",
			"$fi",
			";",
			"",
			"uniform sampler2D texture_albedo : source_color; // $output(0)",
			"uniform sampler2D texture_emission : hint_default_black; // $output(2)",
			"uniform sampler2D texture_normal : hint_normal; // $output(3)",
			"uniform sampler2D texture_orm : hint_default_white; // $output(1)",
			"uniform sampler2D texture_subsurface_scattering : hint_default_white; // $output(5)",
			"uniform sampler2D texture_depth : hint_default_black; // $output(4)",
			"uniform int depth_min_layers = 64;",
			"uniform int depth_max_layers = 64;",
			"uniform vec2 depth_flip = vec2(1.0);",
			"",
			"uniform vec3 uv1_scale;",
			"uniform vec3 uv1_offset;",
			"",
			"$definitions",
			"",
			"void vertex() {",
			"\tUV=UV*uv1_scale.xy+uv1_offset.xy;",
			"}",
			"",
			"void fragment() {",
			"\tvec2 base_uv = UV;",
			"$if $(connected:depth_tex)",
			"\t{",
			"$begin_generate",
			"\t\tfloat depth_scale = 0.2*$depth_scale;",
			"$end_generate",
			"\t\tvec3 view_dir = normalize(normalize(-VERTEX)*mat3(TANGENT*depth_flip.x,-BINORMAL*depth_flip.y,NORMAL));",
			"\t\tfloat num_layers = mix(float(depth_max_layers),float(depth_min_layers), abs(dot(vec3(0.0, 0.0, 1.0), view_dir)));",
			"\t\tfloat layer_depth = 1.0 / num_layers;",
			"\t\tfloat current_layer_depth = 0.0;",
			"\t\tvec2 P = view_dir.xy * depth_scale;",
			"\t\tvec2 delta = P / num_layers / dot(VIEW, NORMAL);",
			"\t\tvec2  ofs = base_uv;",
			"\t\tfloat depth = textureLod(texture_depth, ofs,0.0).r;",
			"\t\tfloat current_depth = 0.0;",
			"\t\twhile(current_depth < depth) {",
			"\t\t\tofs -= delta;",
			"\t\t\tdepth = textureLod(texture_depth, ofs,0.0).r;",
			"\t\t\tcurrent_depth += layer_depth;",
			"\t\t}",
			"\t\tvec2 prev_ofs = ofs + delta;",
			"\t\tfloat after_depth  = depth - current_depth;",
			"\t\tfloat before_depth = textureLod(texture_depth, prev_ofs, 0.0).r - current_depth + layer_depth;",
			"\t\tfloat weight = after_depth / (after_depth - before_depth);",
			"\t\tofs = mix(ofs,prev_ofs,weight);",
			"\t\tbase_uv=ofs;",
			"\t}",
			"$fi",
			"$begin_generate",
			"\tvec4 albedo_tex = texture(texture_albedo, base_uv);",
			"\tALBEDO = $albedo_color.rgb * mix(pow((albedo_tex + vec4(0.055))/vec4(1.055), vec4(2.4)), albedo_tex/vec4(12.92), lessThan(albedo_tex, vec4(0.04045))).rgb;",
			"\tvec4 orm_tex = texture(texture_orm, base_uv);",
			"\tMETALLIC = $metallic*orm_tex.b;",
			"\tROUGHNESS = $roughness*orm_tex.g;",
			"\tSPECULAR = 0.5;",
			"\tNORMAL_MAP = texture(texture_normal, base_uv).rgb;",
			"\tNORMAL_MAP_DEPTH = $normal;",
			"\tvec3 emission_tex = texture(texture_emission, base_uv).rgb;",
			"\tEMISSION = emission_tex*$emission_energy;",
			"\tAO = orm_tex.r;",
			"\tAO_LIGHT_AFFECT = $ao;",
			"$if $(connected:opacity_tex) and $(param:flags_transparent)",
			"\tALPHA = albedo_tex.a;",
			"\tALPHA_SCISSOR_THRESHOLD = 0.01;",
			"$fi",
			"$if $(connected:sss_tex)",
			"\tfloat sss_tex = texture(texture_subsurface_scattering, base_uv).r;",
			"\tSSS_STRENGTH=$sss*sss_tex;",
			"$fi",
			"$end_generate",
			"}",
			""
		]
