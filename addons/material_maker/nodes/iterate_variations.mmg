		],
		"instance": [
			"float iterate_variations_$(name)(vec2 uv, float v1, float v2, float step) {",
			"\tfloat v = 0.0;",
			"\tvec4 var = _controlled_variation_;",
			"\tif (sign(step) == sign(v2-v1)) {",
			"\t\tfor (float x = v1; x < v2; x += step) {",
			"\t\t\tvar.$variable = x;",
			"\t\t\tfloat input = $in.variation(uv, _seed_variation_, var);",
			"\t\t\tv = v+input;",
			"\t\t}",
			"\t}",
			"\treturn v;",
			"}"
		],
		"longdesc": "Combines controlled variations for its input by iterating on the variable",
		"name": "Iterate Variations",
		"outputs": [
			{
				"f": "iterate_variations_$(name)($uv, $v1, $v2, $step)",
				"longdesc": "Shows a variation of the input",
				"shortdesc": "Output1",
				"type": "f"
			}
		],
		"parameters": [
